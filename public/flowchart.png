<svg width="1200" height="600" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="1200" height="600" fill="#000000"/>
  
  <!-- Decorative triangles (background pattern) -->
  <defs>
    <pattern id="triangles" x="0" y="0" width="100" height="100" patternUnits="userSpaceOnUse">
      <polygon points="0,100 50,0 100,100" fill="#991b1b" opacity="0.3"/>
    </pattern>
  </defs>
  <rect width="1200" height="600" fill="url(#triangles)"/>
  
  <!-- Title -->
  <text x="600" y="80" font-family="Arial, sans-serif" font-size="48" font-weight="bold" fill="#EF4444" text-anchor="middle">
    FLOW CHART
  </text>
  
  <!-- CODE M Badge -->
  <rect x="1050" y="30" width="120" height="40" rx="20" fill="#DC2626"/>
  <text x="1110" y="57" font-family="Arial, sans-serif" font-size="18" font-weight="bold" fill="#FFFFFF" text-anchor="middle">
    CODE M
  </text>
  
  <!-- Main flow boxes -->
  
  <!-- Step 1: Smart Application Intake -->
  <rect x="50" y="150" width="200" height="120" rx="10" fill="none" stroke="#8B5CF6" stroke-width="3"/>
  <text x="150" y="195" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="#FFFFFF" text-anchor="middle">
    1. Smart Application
  </text>
  <text x="150" y="220" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="#FFFFFF" text-anchor="middle">
    Intake
  </text>
  
  <!-- Step 2: KYC + Fraud Check -->
  <rect x="50" y="330" width="200" height="150" rx="10" fill="none" stroke="#8B5CF6" stroke-width="3"/>
  <text x="150" y="365" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#FFFFFF" text-anchor="middle">
    2. KYC + Fraud Check
  </text>
  <text x="150" y="395" font-family="Arial, sans-serif" font-size="12" fill="#FFFFFF" text-anchor="middle">
    • OCR extract
  </text>
  <text x="150" y="415" font-family="Arial, sans-serif" font-size="12" fill="#FFFFFF" text-anchor="middle">
    • Face match
  </text>
  <text x="150" y="435" font-family="Arial, sans-serif" font-size="12" fill="#FFFFFF" text-anchor="middle">
    • Liveness
  </text>
  <text x="150" y="455" font-family="Arial, sans-serif" font-size="12" fill="#FFFFFF" text-anchor="middle">
    • Tamper detection
  </text>
  
  <!-- Document icon -->
  <rect x="350" y="170" width="100" height="80" rx="5" fill="none" stroke="#8B5CF6" stroke-width="2"/>
  <text x="400" y="215" font-family="Arial, sans-serif" font-size="12" fill="#FFFFFF" text-anchor="middle">
    Documents
  </text>
  
  <!-- Step 3: Document Extraction -->
  <rect x="550" y="150" width="200" height="120" rx="10" fill="none" stroke="#8B5CF6" stroke-width="3"/>
  <text x="650" y="195" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#FFFFFF" text-anchor="middle">
    Document Extraction
  </text>
  <text x="650" y="220" font-family="Arial, sans-serif" font-size="12" fill="#FFFFFF" text-anchor="middle">
    Salary slip
  </text>
  <text x="650" y="240" font-family="Arial, sans-serif" font-size="12" fill="#FFFFFF" text-anchor="middle">
    Bank Statement
  </text>
  
  <!-- Step 4: Risk Scoring -->
  <rect x="550" y="330" width="200" height="150" rx="10" fill="none" stroke="#8B5CF6" stroke-width="3"/>
  <text x="650" y="365" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#FFFFFF" text-anchor="middle">
    3. Automated Risk
  </text>
  <text x="650" y="385" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#FFFFFF" text-anchor="middle">
    Scoring Engine
  </text>
  <circle cx="650" cy="430" r="30" fill="none" stroke="#8B5CF6" stroke-width="2"/>
  <text x="650" y="440" font-family="Arial, sans-serif" font-size="14" fill="#FFFFFF" text-anchor="middle">
    0-100
  </text>
  
  <!-- Risk levels -->
  <rect x="820" y="180" width="150" height="40" rx="5" fill="#10B981" opacity="0.8"/>
  <text x="895" y="207" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#FFFFFF" text-anchor="middle">
    Low Risk (70-100)
  </text>
  
  <rect x="820" y="240" width="150" height="40" rx="5" fill="#F59E0B" opacity="0.8"/>
  <text x="895" y="267" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#FFFFFF" text-anchor="middle">
    Medium Risk (40-70)
  </text>
  
  <rect x="820" y="300" width="150" height="40" rx="5" fill="#EF4444" opacity="0.8"/>
  <text x="895" y="327" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#FFFFFF" text-anchor="middle">
    High Risk (0-40)
  </text>
  
  <!-- Step 5: Decision -->
  <rect x="320" y="520" width="250" height="60" rx="10" fill="none" stroke="#8B5CF6" stroke-width="3"/>
  <text x="445" y="540" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#FFFFFF" text-anchor="middle">
    4. Automated Lending
  </text>
  <text x="445" y="560" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#FFFFFF" text-anchor="middle">
    Decision
  </text>
  
  <!-- Step 6: Admin Dashboard -->
  <rect x="650" y="520" width="250" height="60" rx="10" fill="none" stroke="#8B5CF6" stroke-width="3"/>
  <text x="775" y="540" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#FFFFFF" text-anchor="middle">
    5. Admin Dashboard
  </text>
  <text x="775" y="560" font-family="Arial, sans-serif" font-size="12" fill="#FFFFFF" text-anchor="middle">
    (Lender View)
  </text>
  
  <!-- Arrows -->
  <defs>
    <marker id="arrowhead" markerWidth="10" markerHeight="10" refX="9" refY="3" orient="auto">
      <polygon points="0 0, 10 3, 0 6" fill="#8B5CF6" />
    </marker>
  </defs>
  
  <!-- Arrow 1 to 2 -->
  <line x1="150" y1="270" x2="150" y2="320" stroke="#8B5CF6" stroke-width="2" marker-end="url(#arrowhead)"/>
  
  <!-- Arrow to document -->
  <line x1="250" y1="210" x2="340" y2="210" stroke="#8B5CF6" stroke-width="2" marker-end="url(#arrowhead)"/>
  
  <!-- Arrow document to extraction -->
  <line x1="450" y1="210" x2="540" y2="210" stroke="#8B5CF6" stroke-width="2" marker-end="url(#arrowhead)"/>
  
  <!-- Arrow extraction to risk -->
  <line x1="650" y1="270" x2="650" y2="320" stroke="#8B5CF6" stroke-width="2" marker-end="url(#arrowhead)"/>
  
  <!-- Arrow risk to levels -->
  <line x1="750" y1="405" x2="810" y2="300" stroke="#8B5CF6" stroke-width="2" marker-end="url(#arrowhead)"/>
  
  <!-- Arrow to decision -->
  <line x1="445" y1="480" x2="445" y2="510" stroke="#8B5CF6" stroke-width="2" marker-end="url(#arrowhead)"/>
  
  <!-- Arrow decision to admin -->
  <line x1="570" y1="550" x2="640" y2="550" stroke="#8B5CF6" stroke-width="2" marker-end="url(#arrowhead)"/>
  
  <!-- Note -->
  <text x="600" y="590" font-family="Arial, sans-serif" font-size="10" fill="#9CA3AF" text-anchor="middle">
    ⚠️ Replace this placeholder with your actual flowchart image at public/flowchart.png
  </text>
</svg>
